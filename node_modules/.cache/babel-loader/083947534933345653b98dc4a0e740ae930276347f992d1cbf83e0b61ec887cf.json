s from 'axios';\r\n  \r\n  export default {\r\n    components: { Master, Loader },\r\n    name: \"ProductPage\",\r\n    data() {\r\n      return {\r\n        isLoad: true,\r\n        products: [],\r\n      };\r\n    },\r\n    created() {\r\n      axios.get('https://raw.githubusercontent.com/SAP/openui5/master/src/sap.m/test/sap/m/demokit/cart/webapp/localService/mockdata/Product.json')\r\n        .then((res) => {\r\n          console.log('Received data:', res.data);\r\n          this.products = res.data;\r\n          this.isLoad = false;\r\n        })\r\n        .catch((error) => {\r\n          console.error('Error fetching data:', error);\r\n          this.isLoad = false;\r\n        });\r\n    },\r\n  };\r\n  </script>\r\n  "],"mappings":";;;;;;;;;;;;;;;;;;;;;;;AAuBA,OAAAA,MAAA;AACA,OAAAC,MAAA;AACA,OAAAC,KAAA;AAEA;EACAC,UAAA;IAAAH,MAAA;IAAAC;EAAA;EACAG,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACAN,KAAA,CAAAO,GAAA,qIACAC,IAAA,CAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,mBAAAF,GAAA,CAAAN,IAAA;MACA,KAAAE,QAAA,GAAAI,GAAA,CAAAN,IAAA;MACA,KAAAC,MAAA;IACA,GACAQ,KAAA,CAAAC,KAAA;MACAH,OAAA,CAAAG,KAAA,yBAAAA,KAAA;MACA,KAAAT,MAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}